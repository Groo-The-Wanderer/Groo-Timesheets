<%= form_for @client do |c| %>
  <div class="form-group">
      <%= c.label(:name, "Name:", class: "control-label col-sm-2") %>
      <div class="col-sm-8"><%= c.text_field :name, class: "form-control" %></div>
  </div>
  <div class="form-group">
      <%= c.label(:prefix, "Project Prefix:", class: "control-label col-sm-2") %>
      <div class="col-sm-8"><%= c.text_field :prefix, class: "form-control" %></div>
  </div>
  <div class="form-group">
    <div class="col-sm-8"><%= c.submit "Submit", class: "btn btn-primary" %></div>
  </div>
<% end %>
