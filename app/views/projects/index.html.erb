<section class="content-header"><h1>Project Maintenance</h1></section>
<section class="content">
  <div class="col-md-12">
    <div class="box">
      <div class="box-header with-border">
        <h3 class="box-title">All Projects</h3>
      </div> <!-- box-header with-border -->
      <div class="box-body">
        <ul>
        <% @projects.each do |p| %>
            <li><%= link_to p.name, project_path( p ) %> (<%= p.client.name %>)</li>
        <% end %>
        </ul>
      </div> <!-- box-body -->
    </div> <!-- box -->
  </div> <!-- col-md-12 -->
</section> <!-- content -->
